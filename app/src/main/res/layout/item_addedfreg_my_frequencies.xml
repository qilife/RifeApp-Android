<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:font="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/dp_5">

    <com.zappkit.zappid.views.CustomFontTextView
        android:id="@+id/mySeqFreqName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_marginEnd="@dimen/dp_10"
        android:paddingTop="@dimen/dp_7"
        android:textColor="@color/color_text"
        android:textSize="@dimen/text_small_14"
        font:fontText="font_helvetica_neue.otf" />

    <ImageView
        android:id="@+id/mySeqFreqDeletebtn"
        android:layout_width="@dimen/dp_30"
        android:layout_height="@dimen/dp_30"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="@dimen/dp_10"
        android:layout_marginEnd="@dimen/dp_5"
        android:layout_marginRight="5dp"
        android:background="@null"
        android:contentDescription="@string/repeatDescript"
        android:focusable="false"
        android:padding="@dimen/dp_5"
        android:src="@drawable/ic_delete" />
</RelativeLayout>
